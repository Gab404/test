{
    "model_type": "diffusion_cond",
    "sample_size": 2097152,
    "sample_rate": 44100,
    "audio_channels": 2,
    "model": {
        "pretransform": {
            "type": "autoencoder",
            "iterate_batch": true,
            "config": {
                "encoder": {
                    "type": "oobleck",
                    "requires_grad": false,
                    "config": {
                        "in_channels": 2,
                        "channels": 128,
                        "c_mults": [1, 2, 4, 8, 16],
                        "strides": [2, 4, 4, 8, 8],
                        "latent_dim": 128,
                        "use_snake": true
                    }
                },
                "decoder": {
                    "type": "oobleck",
                    "config": {
                        "out_channels": 2,
                        "channels": 128,
                        "c_mults": [1, 2, 4, 8, 16],
                        "strides": [2, 4, 4, 8, 8],
                        "latent_dim": 64,
                        "use_snake": true,
                        "final_tanh": false
                    }
                },
                "bottleneck": {
                    "type": "vae"
                },
                "latent_dim": 64,
                "downsampling_ratio": 2048,
                "io_channels": 2
            }
        },
        "conditioning": {
            "configs": [
                {
                    "id": "prompt",
                    "type": "t5",
                    "config": {
                        "t5_model_name": "t5-base",
                        "max_length": 128,
                        "enable_grad": true
                    }
                },
                {
                    "id": "seconds_start",
                    "type": "number",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                },
                {
                    "id": "seconds_total",
                    "type": "number",
                    "config": {
                        "min_val": 0,
                        "max_val": 512
                    }
                }
            ],
            "cond_dim": 768
        },
        "diffusion": {
            "cross_attention_cond_ids": ["prompt", "seconds_start", "seconds_total"],
            "global_cond_ids": ["seconds_start", "seconds_total"],
            "type": "dit",
            "config": {
                "io_channels": 64,
                "embed_dim": 1536,
                "depth": 24,
                "num_heads": 24,
                "cond_token_dim": 768,
                "global_cond_dim": 1536,
                "project_cond_tokens": false,
                "transformer_type": "continuous_transformer"
            }
        },
        "io_channels": 64
    },
    "training": {
        "use_ema": true,
        "log_loss_info": false,
        "optimizer_configs": {
            "diffusion": {
                "optimizer": {
                    "type": "AdamW",
                    "config": {
                        "lr": 5e-5,
                        "betas": [0.9, 0.999],
                        "weight_decay": 1e-3
                    }
                },
                "scheduler": {
                    "type": "InverseLR",
                    "config": {
                        "inv_gamma": 1000000,
                        "power": 0.5,
                        "warmup": 0.99
                    }
                }
            }
        },
        "demo": {
            "demo_every": 2000,
            "demo_steps": 250,
            "num_demos": 4,
            "demo_cond": [
                {"prompt": "A psychedelic Trap beat inspired by Travis Scott, built around swirling synth layers, reversed vocal chops, and heavy 808s. Set in Dâ™¯ minor at 136 BPM, it evokes a hazy, otherworldly vibe.", "seconds_start": 0, "seconds_total": 12},
                {"prompt": "A soulful Trap/R&B fusion beat inspired by Rod Wave, featuring gospel-style piano chords, smooth vocal harmonies, and heartfelt melodic progressions. In the key of G minor at 138 BPM, this beat carries a powerful emotional weight.", "seconds_start": 0, "seconds_total": 160},
                {"prompt": "A gritty, minimal Trap beat inspired by Kodak Black, driven by distorted basslines, sparse percussion, and dark, repetitive melodies. Set at 124 BPM in C minor, the beat creates a raw and streetwise energy.", "seconds_start": 0, "seconds_total": 190},
                {"prompt": "A hard-hitting Trap beat inspired by GloRilla, with punchy drums, gritty bass, and aggressive energy. Set in A minor at 142 BPM.", "seconds_start": 0, "seconds_total": 180}
            ],
            "demo_cfg_scales": [3, 6, 9]
        },
        "lora": {
        "component_whitelist": ["transformer"],
        "multiplier": 1.0,
        "rank": 16,
        "alpha": 16,
        "dropout": 0,
        "module_dropout": 0,
        "lr": 1e-4,
        "weight_decompose": true
        }
    }
}